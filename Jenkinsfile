pipeline{
    agent any
    stages{
        stage('First Stage'){
            steps{
                sh 'echo "My stage 1"'
            }
        }
        stage('Second Stage'){
            steps{
                sh 'echo "My stage 2"'
            }
        }
        stage('Third Stage'){
            steps{
                sh 'echo "My stage 3"'
            }
        }
        stage('Fourth Stage'){
            steps{
                sh 'echo "My final stage"'
            }
        }
    }
}
